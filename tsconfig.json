{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "jsx": "react-jsx",
    "moduleResolution": "Bundler",
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {},
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true
  },
  "include": ["components/**/*", "lib/**/*", "types.ts", "web/**/*"]
}


